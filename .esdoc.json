{
  "source": "./packages/",
  "includes": ["\\w+/src/lib/\\w+\\.js$"],
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "unexportedIdentifier": {
          "enable": true
        }
      }
    },
    {
      "name": "esdoc-publish-markdown-plugin",
      "option": {
        "filename": "README.md"
      }
    },
    {
      "name": "esdoc-flow-plugin"
    }
  ]
}
